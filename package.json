{
  "name": "zlyt-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "theme:custom-dark": "node-sass src/app/theme/custom.scss src/assets/themes/custom-dark.css",
    "theme:dark-pink": "node-sass src/app/theme/dark-pink.scss src/assets/themes/dark-pink.css",
    "theme:teal-purple": "node-sass src/app/theme/teal-purple.scss src/assets/themes/teal-purple.css",
    "build:theme": "npm run theme:custom-dark && npm run theme:dark-pink && npm run theme:teal-purple",
    "preview": "npm run build:prod && docker-compose down && docker-compose up -d",
    "dev:ssr": "ng run zlyt-web:serve-ssr",
    "serve:ssr": "node dist/zlyt-web/server/main.js",
    "build:ssr": "ng build --prod && ng run zlyt-web:server:production",
    "prerender": "ng run zlyt-web:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/material": "^10.2.5",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/platform-server": "~11.0.2",
    "@angular/router": "^11.0.2",
    "@angular/service-worker": "^11.0.2",
    "@nguniversal/express-engine": "^10.1.0",
    "crypto-js": "^4.0.0",
    "express": "^4.15.2",
    "file-saver": "^2.0.2",
    "js-sha3": "^0.8.0",
    "monaco-editor": "^0.21.2",
    "ng-zorro-antd": "^10.0.2",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@nguniversal/builders": "^10.0.1",
    "@types/crypto-js": "^3.1.47",
    "@types/express": "^4.17.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.67",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5"
  }
}
