/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@components": [
        "src/app/components"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@pages": [
        "src/app/pages"
      ],
      "@pages/*": [
        "src/app/pages/*"
      ],
      "@services": [
        "src/app/services"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@utils": [
        "src/app/utils"
      ],
      "@utils/*": [
        "src/app/utils/*"
      ],
      "@api": [
        "src/app/api"
      ],
      "@api/*": [
        "src/app/api/*"
      ],
      "@customTypes": [
        "src/app/customTypes"
      ],
      "@customTypes/*": [
        "src/app/customTypes/*"
      ],
    }
  }
}